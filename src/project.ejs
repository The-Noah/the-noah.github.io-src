<div style="--border-color:<%= project.color %>">
  <h2><%= project.name %></h2>
  <p><%= project.description %>.</p>
  <span style="flex:1"></span>
  <div class="info">
    <div class="labels">
      <% if(project.labels){project.labels.forEach((label) => { %>
        <span><%= label %></span>
      <% })} %>
    </div>
    <div style="flex:1"></div>
