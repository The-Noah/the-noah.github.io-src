<div class="<%= project.small ? "small" : "project" %>" style="--row:<%= project.row %>;--column:<%= project.column %>">
  <% if(!project.small){ %>
    <div class="header">
      <a href="https://<%= project.url %>" target="_blank" rel="noopener noreferrer">View</a>
      <% if(project.github){ %>
        <span style="margin:0 10px">â€¢</span>
        <a href="https://github.com/<%= project.github %>" target="_blank" rel="noopener noreferrer">GitHub</a>
      <% } %>
      <!-- <svg width="20" height="12" viewBox="0 0 20 12" fill="none" xmlns="http://www.w3.org/2000/svg">
        <path d="M1 1L10 10L20 1" stroke="white" stroke-width="2"/>
      </svg> -->
    </div>
  <% } %>
  <%- project.logo %>
  <div class="info">
    <h2><%= project.name %></h2>
    <p><%= project.description %></p>
  </div>
</div>
